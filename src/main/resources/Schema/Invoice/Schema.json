          }
        }
      }
    },
    {
      "if": {
        "properties": {
          "documentCurrencyCode": {
            "const": "AED"
          }
        }
      },
      "then": {
        "not": {
          "required": [
            "taxExchangeRate"
          ]
        },
        "properties": {
          "taxExchangeRate": {
            "not": {
              "required": [
                "calculationRate"
              ]
            }
          }
        }
      }
    },
    {
      "if": {
        "properties": {
          "profileExecutionID": {
